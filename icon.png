        }
        categoryMap[entry.</span><span><span class="hljs-property">website</span></span><span>][entry.</span><span><span class="hljs-property">category</span></span><span>] = entry.</span><span><span class="hljs-property">totalTime</span></span><span>;
      });

      </span><span><span class="hljs-keyword">for</span></span><span> (</span><span><span class="hljs-keyword">const</span></span><span> [site, times] </span><span><span class="hljs-keyword">of</span></span><span> </span><span><span class="hljs-title class_">Object</span></span><span>.</span><span><span class="hljs-title function_">entries</span></span><span>(categoryMap)) {
        labels.</span><span><span class="hljs-title function_">push</span></span><span>(site);
        productive.</span><span><span class="hljs-title function_">push</span></span><span>(times.</span><span><span class="hljs-property">productive</span></span><span> || </span><span><span class="hljs-number">0</span></span><span>);
        unproductive.</span><span><span class="hljs-title function_">push</span></span><span>(times.</span><span><span class="hljs-property">unproductive</span></span><span> || </span><span><span class="hljs-number">0</span></span><span>);
      }

      </span><span><span class="hljs-keyword">const</span></span><span> ctx = </span><span><span class="hljs-variable language_">document</span></span><span>.</span><span><span class="hljs-title function_">getElementById</span></span><span>(</span><span><span class="hljs-string">"usageChart"</span></span><span>).</span><span><span class="hljs-title function_">getContext</span></span><span>(</span><span><span class="hljs-string">"2d"</span></span><span>);

      </span><span><span class="hljs-keyword">new</span></span><span> </span><span><span class="hljs-title class_">Chart</span></span><span>(ctx, {
        </span><span><span class="hljs-attr">type</span></span><span>: </span><span><span class="hljs-string">"bar"</span></span><span>,
        </span><span><span class="hljs-attr">data</span></span><span>: {
          labels,
          </span><span><span class="hljs-attr">datasets</span></span><span>: [
            {
              </span><span><span class="hljs-attr">label</span></span><span>: </span><span><span class="hljs-string">"Productive"</span></span><span>,
              </span><span><span class="hljs-attr">data</span></span><span>: productive,
