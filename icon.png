const express = require('express');
const mysql = require('mysql2');
const cors = require('cors');
const app = express();
const PORT = 5000;

app.use(cors());
app.use(express.json());

const db = mysql.createConnection({
  host: 'localhost',
  user: 'root',
  password: '1432580G',
  database: 'time_tracker'
});

db.connect((err) =&gt; {
  if (err) {
    console.error('Database connection failed:', err);
  } else {
    console.log('Connected to MySQL');
  }
});

function classify(url) {
  const productiveKeywords = ['leetcode', 'github', 'stackoverflow', 'geeksforgeeks'];
  return productiveKeywords.some(keyword =&gt; url.includes(keyword)) ? 'productive' : 'unproductive';
}

app.post('/track', (req, res) =&gt; {
  const { url, duration } = req.body;
  if (!url || !duration) return res.status(400).send('Missing url or duration');
  const domain = new URL(url).hostname;
  const category = classify(url);

 db.query(
  'INSERT INTO usage_data (url, domain, duration, category) VALUES (?, ?, ?, ?)',

    [url, domain, Math.round(duration / 60000), category],
    (err) =&gt; {
      if (err) {
        console.error(err);
        return res.status(500).send('Failed to store data');
      }
      res.send('Data stored');
    }
  );
});

app.get('/report', (req, res) =&gt; {
  db.query(
  <code>SELECT domain AS website, SUM(duration) AS totalTime, category
</code>   FROM usage_data
   WHERE timestamp &gt;= NOW() - INTERVAL 7 DAY
   GROUP BY domain, category<code>,
</code>
    (err, result) =&gt; {
      if (err) {
        console.error(err);
        return res.status(500).send('Failed to fetch report');
      }
      res.json(result);
    }
  );
