  </span><span><span class="hljs-title function_">fetch</span></span><span>(</span><span><span class="hljs-string">"http://localhost:5000/report"</span></span><span>)
    .</span><span><span class="hljs-title function_">then</span></span><span>(</span><span><span class="hljs-function"><span class="hljs-params">res</span></span></span><span> =&gt; res.</span><span><span class="hljs-title function_">json</span></span><span>())
    .</span><span><span class="hljs-title function_">then</span></span><span>(</span><span><span class="hljs-function"><span class="hljs-params">data</span></span></span><span> =&gt; {
      </span><span><span class="hljs-keyword">const</span></span><span> productive = data.</span><span><span class="hljs-property">productive</span></span><span> || </span><span><span class="hljs-number">0</span></span><span>;
      </span><span><span class="hljs-keyword">const</span></span><span> unproductive = data.</span><span><span class="hljs-property">unproductive</span></span><span> || </span><span><span class="hljs-number">0</span></span><span>;
      </span><span><span class="hljs-keyword">const</span></span><span> total = productive + unproductive;

      </span><span><span class="hljs-keyword">const</span></span><span> ctx = </span><span><span class="hljs-variable language_">document</span></span><span>.</span><span><span class="hljs-title function_">getElementById</span></span><span>(</span><span><span class="hljs-string">"productivityChart"</span></span><span>).</span><span><span class="hljs-title function_">getContext</span></span><span>(</span><span><span class="hljs-string">"2d"</span></span><span>);

      </span><span><span class="hljs-keyword">new</span></span><span> </span><span><span class="hljs-title class_">Chart</span></span><span>(ctx, {
