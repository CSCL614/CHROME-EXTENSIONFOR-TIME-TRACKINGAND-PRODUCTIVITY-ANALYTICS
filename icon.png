            </span><span><span class="hljs-attr">data</span></span><span>: productiveData,
            </span><span><span class="hljs-attr">backgroundColor</span></span><span>: </span><span><span class="hljs-string">'rgba(0, 123, 255, 0.6)'</span></span><span>
