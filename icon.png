            },
            {
              </span><span><span class="hljs-attr">label</span></span><span>: </span><span><span class="hljs-string">"Unproductive"</span></span><span>,
              </span><span><span class="hljs-attr">data</span></span><span>: unproductive,
              </span><span><span class="hljs-attr">backgroundColor</span></span><span>: </span><span><span class="hljs-string">"#F44336"</span></span><span>
            }
          ]
        },
        </span><span><span class="hljs-attr">options</span></span><span>: {
          </span><span><span class="hljs-attr">responsive</span></span><span>: </span><span><span class="hljs-literal">true</span></span><span>,
          </span><span><span class="hljs-attr">plugins</span></span><span>: {
            </span><span><span class="hljs-attr">legend</span></span><span>: { </span><span><span class="hljs-attr">position</span></span><span>: </span><span><span class="hljs-string">"top"</span></span><span> },
