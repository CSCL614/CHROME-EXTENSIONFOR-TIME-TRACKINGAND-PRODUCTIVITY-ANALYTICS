            </span><span><span class="hljs-attr">label</span></span><span>: </span><span><span class="hljs-string">'Unproductive (min)'</span></span><span>,
            </span><span><span class="hljs-attr">data</span></span><span>: unproductiveData,
            </span><span><span class="hljs-attr">backgroundColor</span></span><span>: </span><span><span class="hljs-string">'rgba(255, 99, 132, 0.6)'</span></span><span>
          }
        ]
      },
      </span><span><span class="hljs-attr">options</span></span><span>: {
        </span><span><span class="hljs-attr">responsive</span></span><span>: </span><span><span class="hljs-literal">true</span></span><span>,
        </span><span><span class="hljs-attr">scales</span></span><span>: {
          </span><span><span class="hljs-attr">y</span></span><span>: {
