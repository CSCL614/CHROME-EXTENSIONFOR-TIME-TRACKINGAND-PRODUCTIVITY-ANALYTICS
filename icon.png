          </span><span><span class="hljs-attr">datasets</span></span><span>: [{
            </span><span><span class="hljs-attr">data</span></span><span>: [productive, unproductive],
            </span><span><span class="hljs-attr">backgroundColor</span></span><span>: [</span><span><span class="hljs-string">"#4CAF50"</span></span><span>, </span><span><span class="hljs-string">"#F44336"</span></span><span>]
          }]
        }
      });

      </span><span><span class="hljs-keyword">const</span></span><span> percentage = total === </span><span><span class="hljs-number">0</span></span><span> ? </span><span><span class="hljs-number">0</span></span><span> : </span><span><span class="hljs-title class_">Math</span></span><span>.</span><span><span class="hljs-title function_">round</span></span><span>((productive / total) * </span><span><span class="hljs-number">100</span></span><span>);
      </span><span><span class="hljs-variable language_">document</span></span><span>.</span><span><span class="hljs-title function_">getElementById</span></span><span>(</span><span><span class="hljs-string">"summary"</span></span><span>).</span><span><span class="hljs-property">innerText</span></span><span> = </span><span><span class="hljs-string">`Productivity: <span class="hljs-subst">${percentage}</span></span></span><span>%`;
    })
    .</span><span><span class="hljs-title function_">catch</span></span><span>(</span><span><span class="hljs-function"><span class="hljs-params">err</span></span></span><span> =&gt; {
      </span><span><span class="hljs-variable language_">console</span></span><span>.</span><span><span class="hljs-title function_">error</span></span><span>(</span><span><span class="hljs-string">"Failed to fetch report"</span></span><span>, err);
      </span><span><span class="hljs-variable language_">document</span></span><span>.</span><span><span class="hljs-title function_">getElementById</span></span><span>(</span><span><span class="hljs-string">"summary"</span></span><span>).</span><span><span class="hljs-property">innerText</span></span><span> = </span><span><span class="hljs-string">"Unable to fetch data."</span></span><span>;
    });
});
</span></span></code></div></div></pre>
