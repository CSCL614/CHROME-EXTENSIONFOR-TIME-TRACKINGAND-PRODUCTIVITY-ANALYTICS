            }
          }
        }
      }
    });

    </span><span><span class="hljs-comment">// Summary text</span></span><span>
    </span><span><span class="hljs-keyword">const</span></span><span> totalProd = productiveData.</span><span><span class="hljs-title function_">reduce</span></span><span>(</span><span><span class="hljs-function">(<span class="hljs-params">a, b</span></span></span><span>) =&gt; a + b, </span><span><span class="hljs-number">0</span></span><span>);
    </span><span><span class="hljs-keyword">const</span></span><span> totalUnprod = unproductiveData.</span><span><span class="hljs-title function_">reduce</span></span><span>(</span><span><span class="hljs-function">(<span class="hljs-params">a, b</span></span></span><span>) =&gt; a + b, </span><span><span class="hljs-number">0</span></span><span>);
    </span><span><span class="hljs-keyword">const</span></span><span> total = totalProd + totalUnprod;

    summary.</span><span><span class="hljs-property">textContent</span></span><span> = </span><span><span class="hljs-string">`In the last 7 days, you spent <span class="hljs-subst">${totalProd}</span></span></span><span> minutes on productive websites and </span><span><span class="hljs-subst">${totalUnprod}</span></span><span> minutes on unproductive ones (</span><span><span class="hljs-subst">${total}</span></span><span> minutes total).`;

  } </span><span><span class="hljs-keyword">catch</span></span><span> (error) {
