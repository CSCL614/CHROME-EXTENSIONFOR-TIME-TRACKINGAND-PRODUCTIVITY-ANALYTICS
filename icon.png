          }
        }
      });

      </span><span><span class="hljs-keyword">const</span></span><span> totalTime = [...productive, ...unproductive].</span><span><span class="hljs-title function_">reduce</span></span><span>(</span><span><span class="hljs-function">(<span class="hljs-params">a, b</span></span></span><span>) =&gt; a + b, </span><span><span class="hljs-number">0</span></span><span>);
      </span><span><span class="hljs-keyword">const</span></span><span> totalProductive = productive.</span><span><span class="hljs-title function_">reduce</span></span><span>(</span><span><span class="hljs-function">(<span class="hljs-params">a, b</span></span></span><span>) =&gt; a + b, </span><span><span class="hljs-number">0</span></span><span>);
      </span><span><span class="hljs-keyword">const</span></span><span> percent = totalTime === </span><span><span class="hljs-number">0</span></span><span> ? </span><span><span class="hljs-number">0</span></span><span> : </span><span><span class="hljs-title class_">Math</span></span><span>.</span><span><span class="hljs-title function_">round</span></span><span>((totalProductive / totalTime) * </span><span><span class="hljs-number">100</span></span><span>);

      </span><span><span class="hljs-variable language_">document</span></span><span>.</span><span><span class="hljs-title function_">getElementById</span></span><span>(</span><span><span class="hljs-string">"summary"</span></span><span>).</span><span><span class="hljs-property">innerText</span></span><span> = </span><span><span class="hljs-string">`Overall Productivity: <span class="hljs-subst">${percent}</span></span></span><span>%`;
    })
