</span><span><span class="hljs-keyword">const</span></span><span> db = mysql.</span><span><span class="hljs-title function_">createConnection</span></span><span>({
  </span><span><span class="hljs-attr">host</span></span><span>: </span><span><span class="hljs-string">"localhost"</span></span><span>,
  </span><span><span class="hljs-attr">user</span></span><span>: </span><span><span class="hljs-string">"root"</span></span><span>,
  </span><span><span class="hljs-attr">password</span></span><span>: </span><span><span class="hljs-string">"your_password"</span></span><span>,
  </span><span><span class="hljs-attr">database</span></span><span>: </span><span><span class="hljs-string">"time_tracker"</span></span><span>
});

</span><span><span class="hljs-keyword">function</span></span><span> </span><span><span class="hljs-title function_">classify</span></span><span>(</span><span><span class="hljs-params">url</span></span><span>) {
  </span><span><span class="hljs-keyword">if</span></span><span> (url.</span><span><span class="hljs-title function_">includes</span></span><span>(</span><span><span class="hljs-string">"leetcode"</span></span><span>) || url.</span><span><span class="hljs-title function_">includes</span></span><span>(</span><span><span class="hljs-string">"github"</span></span><span>) || url.</span><span><span class="hljs-title function_">includes</span></span><span>(</span><span><span class="hljs-string">"geeksforgeeks"</span></span><span>)) {
    </span><span><span class="hljs-keyword">return</span></span><span> </span><span><span class="hljs-string">"productive"</span></span><span>;
  }
  </span><span><span class="hljs-keyword">return</span></span><span> </span><span><span class="hljs-string">"unproductive"</span></span><span>;
}

app.</span><span><span class="hljs-title function_">post</span></span><span>(</span><span><span class="hljs-string">'/track'</span></span><span>, </span><span><span class="hljs-function">(<span class="hljs-params">req, res</span></span></span><span>) =&gt; {
  </span><span><span class="hljs-keyword">const</span></span><span> { url, duration } = req.</span><span><span class="hljs-property">body</span></span><span>;
  </span><span><span class="hljs-keyword">const</span></span><span> domain = </span><span><span class="hljs-keyword">new</span></span><span> </span><span><span class="hljs-title function_">URL</span></span><span>(url).</span><span><span class="hljs-property">hostname</span></span><span>;
  </span><span><span class="hljs-keyword">const</span></span><span> category = </span><span><span class="hljs-title function_">classify</span></span><span>(url);
  db.</span><span><span class="hljs-title function_">query</span></span><span>(
    </span><span><span class="hljs-string">"INSERT INTO usage (url, domain, duration, category) VALUES (?, ?, ?, ?)"</span></span><span>,
    [url, domain, </span><span><span class="hljs-title class_">Math</span></span><span>.</span><span><span class="hljs-title function_">round</span></span><span>(duration / </span><span><span class="hljs-number">60000</span></span><span>), category],
    </span><span><span class="hljs-function">(<span class="hljs-params">err</span></span></span><span>) =&gt; {
      </span><span><span class="hljs-keyword">if</span></span><span> (err) </span><span><span class="hljs-keyword">return</span></span><span> res.</span><span><span class="hljs-title function_">status</span></span><span>(</span><span><span class="hljs-number">500</span></span><span>).</span><span><span class="hljs-title function_">send</span></span><span>(err);
      res.</span><span><span class="hljs-title function_">send</span></span><span>(</span><span><span class="hljs-string">"Data stored"</span></span><span>);
    }
  );
});

app.</span><span><span class="hljs-title function_">get</span></span><span>(</span><span><span class="hljs-string">'/report'</span></span><span>, </span><span><span class="hljs-function">(<span class="hljs-params">req, res</span></span></span><span>) =&gt; {
  db.</span><span><span class="hljs-title function_">query</span></span><span>(
    <span class="hljs-string">`SELECT domain AS website, SUM(duration) AS totalTime, category
     FROM usage
     WHERE timestamp &gt;= NOW() - INTERVAL 7 DAY
     GROUP BY domain, category`</span>,
    </span><span><span class="hljs-function">(<span class="hljs-params">err, result</span></span></span><span>) =&gt; {
      </span><span><span class="hljs-keyword">if</span></span><span> (err) </span><span><span class="hljs-keyword">return</span></span><span> res.</span><span><span class="hljs-title function_">status</span></span><span>(</span><span><span class="hljs-number">500</span></span><span>).</span><span><span class="hljs-title function_">send</span></span><span>(err);
      res.</span><span><span class="hljs-title function_">json</span></span><span>(result);
    }
  );
});

app.</span><span><span class="hljs-title function_">listen</span></span><span>(</span><span><span class="hljs-number">5000</span></span><span>, </span><span><span class="hljs-function">() =&gt;</span></span><span> </span><span><span class="hljs-variable language_">console</span></span><span>.</span><span><span class="hljs-title function_">log</span></span><span>(</span><span><span class="hljs-string">"Server running on port 5000"</span></span><span>));
